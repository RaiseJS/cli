  "license": "GPL-3.0-or-later",
  "preferGlobal": true,
  "readme": "./license.txt",
  "dependencies": {
    "colors": "^1.3.3",
    "enquirer": "^2.3.0",
    "mustache": "^3.0.1",
    "rimraf": "^2.6.3"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "scripts": {},
  "main": "./lib/localinstall.js",
